<thead class="block mb-2">
    <tr class="custom-table <%= trStyles %> bg-[#e2e8f0] rounded-lg flex w-full gap-x-4">
      <% headers.forEach(header => { %>
        <th class="<%= header.styles %>">
          <% if (header.name === '') { %>
            <i class="fa-regular fa-square-minus"></i>
          <% } else { %>
            <%= header.name %>
          <% } %>
        </th>
      <% }); %>
    </tr>
  </thead>